{
  "name": "@kaave/playground-react",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaave/playground-react.git"
  },
  "license": "MIT",
  "author": "kaave <junkjunctions@gmail.com>",
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "browser": "dist/iife.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/style.css",
  "scripts": {
    "build": "run-s clean build:scripts build:types build:styles",
    "build-storybook": "build-storybook",
    "build:scripts": "rollup -c",
    "build:styles": "sass src/style.scss | postcss > dist/style.css",
    "build:types": "tsc --emitDeclarationOnly -p ./tsconfig.production.json",
    "clean": "rimraf dist",
    "fix": "run-p fix:*",
    "fix:json": "prettier --write '**/*.json'",
    "fix:script": "eslint --fix '**/*.{ts,tsx}'",
    "fix:style": "stylelint --fix '**/*.scss'",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "lint:script": "eslint '**/*.{ts,tsx}'",
    "lint:style": "stylelint '**/*.scss'",
    "release": "np",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest",
    "typecheck": "tsc --noEmit -p ./tsconfig.production.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p typecheck lint-staged"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",

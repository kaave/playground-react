{
  "name": "react-rollup",
  "version": "1.0.0",
  "license": "MIT",
  "author": "kaave <junkjunctions@gmail.com>",
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "browser": "dist/iife.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "run-p build:*",
    "build:scripts": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly -p ./tsconfig.production.json",
    "fix": "run-p fix:*",
    "fix:json": "prettier --write '**/*.json'",
    "fix:script": "eslint --fix '**/*.{ts,tsx}'",
    "fix:style": "stylelint --fix '**/*.scss'",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "lint:script": "eslint '**/*.{ts,tsx}'",
    "lint:style": "stylelint '**/*.scss'",
    "typecheck": "tsc --noEmit -p ./tsconfig.production.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p typecheck lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "autoprefixer": "^9.7.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-redux": "^3.0.3",
    "eslint-plugin-unicorn": "^16.0.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-properties": "^9.0.2",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-image-set-function": "^3.0.1",
    "postcss-initial": "^3.0.2",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "sort-package-json": "^1.39.1",
    "stylelint": "^13.1.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "typescript": "^3.7.5"
  }
}
